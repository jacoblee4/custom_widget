// Setting Overdraft
var type = fd_data.subflow_inputs.signature_parent_record.request_type;

if (type == 'remove_authority')
	return '$0';

var external = fd_data.subflow_inputs.credit_record.overdraft_external_accounts;
var internal = fd_data.subflow_inputs.credit_record.overdraft_internal_accounts;

var externalValue = parseFloat(external.getValue());
var internalValue = parseFloat(internal.getValue());

var largerValue = (externalValue >= internalValue) ? external.getDisplayValue() : internal.getDisplayValue();

// Remove .00 from the display value
largerValue = largerValue.replace(/\.00$/, '');

return largerValue;


// Setting Unavailable
var type = fd_data.subflow_inputs.signature_parent_record.request_type;

if (type == 'remove_authority')
	return '$0';

var external = fd_data.subflow_inputs.credit_record.unavailable_funds_external_accounts;
var internal = fd_data.subflow_inputs.credit_record.unavailable_funds_internal_accounts;

var externalValue = parseFloat(external);
var internalValue = parseFloat(internal);

var largerValue = (externalValue >= internalValue) ? external.getDisplayValue() : internal.getDisplayValue();

// Remove .00 from the display value
largerValue = largerValue.replace(/\.00$/, '');

return largerValue;


// setting wire transfer
var type = fd_data.subflow_inputs.signature_parent_record.request_type;

if (type == 'remove_authority')
	return '$0';

var external = fd_data.subflow_inputs.credit_record.unavailable_funds_external_accounts;
var internal = fd_data.subflow_inputs.credit_record.unavailable_funds_internal_accounts;

var externalValue = parseFloat(external);
var internalValue = parseFloat(internal);

var largerValue = (externalValue >= internalValue) ? external.getDisplayValue() : internal.getDisplayValue();

// Remove .00 from the display value
largerValue = largerValue.replace(/\.00$/, '');

return largerValue;
